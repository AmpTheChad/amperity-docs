/* -- the background color for the whole page and the borders for the topic page, header, footer */
/* -- IMPORTANT -- color is the color of the text on div.document; must be $amperity-brand-slate or $amperity-brand-slate */
body {
  font-family: "Roboto", Arial, sans-serif;
  font-weight: 400;
  font-size: 12px;
  letter-spacing: -0.00em;
  line-height: 1.5;
  margin-left: 0px;
  background-color: #ffffff;
  color: #0c0c0c;
  width: 100%;
  -webkit-font-smoothing: antialiased;
  overflow-x: hidden;
  min-width: 270px; }

/* -- the actual topic background, must be white all the time no exceptions */
div.document {
  background-color: #ffffff;
  text-align: left;
  background-image: url(contents.png);
  background-repeat: repeat-x;
  max-width: 1295px;
  margin: 0 auto;
  width: 100%; }

/* -- the vertical line between the topics and the TOC */
div.bodywrapper {
  margin: 0; }
  @media (min-width: 1100px) {
    div.bodywrapper {
      margin-right: 10px; } }
  div.bodywrapper img {
    max-width: 100%; }

div.documentwrapper {
  background: #ffffff;
  padding-left: 0px; }

div.body-wrap {
  width: 100%;
  overflow: hidden;
  background: white; }

div.body {
  margin: 0;
  padding: 0em 0px 0px 0px; }

div.related {
  font-size: 1em; }

/* -- the borders above and below the header and footer */
ul {
  padding-left: 12px; }

ol {
  padding-left: 12px; }

